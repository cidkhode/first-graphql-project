    "apollo-server-express": "^2.19.0",
    "axios": "^0.19.2",
    "convert-csv-to-json": "^1.0.0",
    "cypress": "^6.0.0",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.1",
    "react-apollo": "^3.1.5",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.4.4",
    "react-table-6": "^6.11.0"
  },
  "devDependencies": {
