    "verbose": true,
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupEnzyme.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testPathIgnorePatterns": ["<rootDir>/cypress/", "<rootDir>/node_modules/"],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/tests/styleMock.js"
    }
  },
  "keywords": [],
  "author": "Chidanand Khode",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
