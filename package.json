{
  "name": "first-graphql-project",
  "version": "1.0.0",
  "description": "Trying out GraphQL for the first time!",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack --mode production",
    "server": "nodemon src/main/back/server.js",
    "cypress": "cypress open",
    "test": "jest"
  },
  "browser": {
    "child_process": false
  },
