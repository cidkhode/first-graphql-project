    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "less-plugin-autoprefix": "^2.0.0",
    "nodemon": "^2.0.6",
